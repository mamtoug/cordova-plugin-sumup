{
  "name": "cordova-sumup-plugin",
  "description": "Cordova plugin for SumUp SDK integration",
  "author": "Marius Backes",
  "license": "MIT",
  "module": "./www/sumup.js",
  "version": "2.0.0",
  "homepage": "https://github.com/mariusbackes/cordova-plugin-sumup",
  "repository": {
    "type": "git",
    "url": "https://github.com/mariusbackes/cordova-plugin-sumup.git"
  },
  "bugs": {
    "url": "https://github.com/mariusbackes/cordova-plugin-sumup.git/issues"
  },
  "cordova": {
    "id": "cordova-sumup-plugin",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "keywords": [
    "sumup",
    "creditcard",
    "debitcard",
    "nfc",
    "card",
    "terminal",
    "pay"
  ],
  "engines": {
    "cordova": ">=6.3.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "Release v${version}",
      "tagAnnotation": "Release v${version}",
      "tagName": "@${version}"
    },
    "npm": {
      "publish": false
    }
  },
  "typings": "./typings/sumup.d.ts"
}
